for x in range(100):
  if (x+1)%3==0:
    print(x+1)

TERCER CÓDIGO:
# -*- coding: utf-8 -*-
"""Untitled0.ipynb

Automatically generated by Colaboratory.

Original file is located at
    https://colab.research.google.com/drive/1a5CY9gIg1M9PUoFaiuiQ_sFcP7bK0iXv
"""

numero1 = int(input("Ingrese el primer número: "))
numero2 = 5

suma= numero1+numero2
resultado = ""

if suma < 100:
    resultado="El resultado es menor a 100"
elif (suma <= 100 and suma <150):
    resultado="El resultado es mayor a 100"
elif (suma >= 150):
    resultado="El resultado es mayor a 150"

print(resultado)

CUARTO CÓDIGO:
# -*- coding: utf-8 -*-
"""Untitled0.ipynb

Automatically generated by Colaboratory.

Original file is located at
    https://colab.research.google.com/drive/1a5CY9gIg1M9PUoFaiuiQ_sFcP7bK0iXv
"""

def evaluar_usuario():
    edad = int(input("¿Cuántos años tienes? "))
    si_programacion = input("¿Te gusta la programación?: ").strip().lower()
    if edad >= 18 and si_programacion == "sí":
        print("Eres mayor de edad y te gusta programar")
    elif edad >= 18 and si_programacion == "no":
        print("Eres mayor de edad, pero no te gusta programar")
    elif edad < 18 and si_programacion == "sí":
        print("Eres menor de edad y te gusta programar")
    else:
        print("Eres menor de edad y no te gusta programar")
evaluar_usuario()
